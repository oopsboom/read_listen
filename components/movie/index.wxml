<view bind:tap="onGoToDetail" class="container">
  <image binderror="getSrc" class="poster" src="{{movie.images.large}}"></image>

  <text class="title">{{movie.title}}</text>
  <view class="rate-container">
    <l-rate disabled size="22" score="{{movie.rating.stars/10}}"></l-rate>
    <text class="score">{{movie.rating.average}}</text>
  </view>

</view>